{
  "name": "angular-vanta-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "esbuild": "ng run angular-vanta-app:esbuild",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "push": "ng build --configuration production --base-href=/angular-vanta-app/ && npx angular-cli-ghpages --dir=dist/angular-vanta-app",
    "push:esbuild": "ng run angular-vanta-app:esbuild --base-href=/angular-vanta-app/ && npx angular-cli-ghpages --dir=dist/angular-vanta-app-esbuild",
    "push:ci": "ng deploy --no-silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "rxjs": "~7.5.0",
    "three": "0.124.0",
    "tslib": "^2.3.0",
    "vanta": "^0.5.22",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/three": "0.125.3",
    "angular-cli-ghpages": "^1.0.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}
